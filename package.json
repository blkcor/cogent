{
  "name": "cogent",
  "version": "1.0.0",
  "description": "AI-powered coding agent with advanced reasoning capabilities",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "cogent": "dist/cli.js"
  },
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "tsc",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["ai", "coding-agent", "llm", "reasoning", "automation"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ai": "^4.0.0",
    "@ai-sdk/anthropic": "^1.0.0",
    "@ai-sdk/openai": "^1.0.0",
    "@ai-sdk/google": "^1.0.0",
    "commander": "^12.0.0",
    "zod": "^3.22.4",
    "ink": "^5.0.1",
    "react": "^18.3.1",
    "ink-spinner": "^5.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "diff": "^7.0.0",
    "fast-glob": "^3.3.2",
    "tree-sitter": "^0.21.1",
    "tree-sitter-typescript": "^0.21.2",
    "tree-sitter-javascript": "^0.21.4",
    "tree-sitter-python": "^0.21.0",
    "uuid": "^10.0.0",
    "dotenv": "^16.4.5",
    "js-tiktoken": "^1.0.12",
    "vectordb": "^0.8.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.1",
    "@types/diff": "^5.2.3",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.6.3",
    "tsx": "^4.19.2",
    "vitest": "^2.1.5",
    "@vitest/ui": "^2.1.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blkcor/cogent.git"
  },
  "types": "./dist/index.d.ts",
  "bugs": {
